{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "delete-tsconfig": "del /f /q tsconfig.json",
    "dev": "yarn delete-tsconfig && concurrently \"next dev --turbo\" \"tailwindcss -i app/globals.css -o app/dist.css -w\"",
    "build": "tailwindcss -m -i app/globals.css -o app/dist.css && next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write app pages components"
  },
  "dependencies": {
    "@nivo/bar": "^0.80.0",
    "@nivo/core": "^0.80.0",
    "@nivo/pie": "^0.80.0",
    "@nivo/scatterplot": "^0.80.0",
    "@nivo/tooltip": "^0.80.0",
    "@tanstack/react-table": "8.7.0",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "eslint": "8.30.0",
    "eslint-config-next": "13.0.6",
    "highcharts": "^10.3.2",
    "highcharts-more": "^0.1.7",
    "highcharts-react-official": "^3.1.0",
    "next": "13.0.6",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "regression": "^2.0.1",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "tailwindcss": "^3.2.4"
  },
  "prettier": {
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "importOrder": [
      "^react",
      "^next",
      "^@",
      "^#",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
}
